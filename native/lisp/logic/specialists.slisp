    (DEFINE-FUNCTION-OBJECT "GET-SYNONYMS"
     "(DEFUN (GET-SYNONYMS (CONS OF LOGIC-OBJECT)) ((TERM OBJECT)))"
     (CL:FUNCTION GET-SYNONYMS) NULL)
    (DEFINE-FUNCTION-OBJECT "SYNONYM-SURROGATE?"
