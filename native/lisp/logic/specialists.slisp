(CL:DEFUN /PL-KERNEL-KB/STANDARD-DEVIATION-SPECIALIST (FRAME LASTMOVE)
  (COMPUTE-VARIANCE-OR-STANDARD-DEVIATION FRAME LASTMOVE TRUE))

